{
  "name": "exhibit-plugin-browserify",
  "version": "0.1.1",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "bluebird": "^3.3.4",
    "browser-resolve-noio": "^1.4.1",
    "browserify": "^13.0.0",
    "defined": "^1.0.0",
    "graceful-fs": "^4.1.3",
    "has": "^1.0.1",
    "insert-module-globals": "^7.0.1",
    "lazy-builder": "^0.1.1",
    "lodash": "^4.7.0",
    "micromatch": "^2.3.7",
    "module-deps-diskless": "^3.8.1",
    "streamifier": "^0.1.1",
    "subdir": "0.0.3",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "del-cli": "^0.2.0",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-babel": "^3.1.0",
    "exhibit": "^0.8.3",
    "test-exhibit-plugin": "^0.1.2"
  },
  "engines": {
    "node": ">=0.12"
  },
  "eslintConfig": {
    "extends": "airbnb/base",
    "root": "true",
    "parser": "babel-eslint",
    "rules": {
      "indent": [
        2,
        "tab"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "peerDependencies": {
    "exhibit": "^0.8.3"
  },
  "repository": "exhibitjs/exhibit-plugin-browserify",
  "scripts": {
    "build": "del dist && babel src --out-dir dist --source-maps",
    "build:watch": "del dist && babel src --out-dir dist --watch --source-maps",
    "prepublish": "npm run build",
    "test": "test-exhibit-plugin"
  }
}
